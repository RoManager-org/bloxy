{
  "name": "@romanager/bloxy",
  "private": false,
  "version": "6.0.3",
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "./dist/index.js",
  "description": "A Node.JS library for interacting with the Roblox website and its API.",
  "bugs": {
    "url": "https://github.com/RoManager-org/bloxy/issues"
  },
  "repository": {
    "directory": "https://github.com",
    "url": "https://github.com/RoManager-org/bloxy",
    "type": "github"
  },
  "homepage": "https://bloxy.js.org/",
  "scripts": {
    "test": "jest",
    "build": "tsc --skipLibCheck",
    "eslint": "eslint . --ext .ts",
    "prepublishOnly": "pnpm run build",
    "create-docs": "typedoc --plugin typedoc-plugin-markdown --hideBreadcrumbs --namedAnchors --excludeExternals --out docs src && mkdocs build",
    "prepare-windows": "pnpm install && pnpm run build && pnpm run create-docs && echo 'Finished'"
  },
  "types": "./dist/index.d.ts",
  "keywords": [
    "roblox",
    "roblox api",
    "roblox group",
    "roblox groups",
    "bloxy",
    "nodejs roblox",
    "roblox nodejs",
    "roblox module"
  ],
  "author": "ScriptBased",
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/node": "^18.6.3",
    "@types/tough-cookie": "^4.0.2",
    "@types/websocket": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-plugin-security": "^1.5.0",
    "faker": "^6.6.6",
    "jest": "^28.1.3",
    "joi": "^17.6.0",
    "typedoc": "^0.23.10",
    "typedoc-plugin-markdown": "^3.13.4",
    "typedoc-plugin-markdown-pages": "^0.3.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "got": "^11.8.5",
    "signalr-client": "0.0.20",
    "tough-cookie": "^4.0.0",
    "tslib": "^2.4.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/"
  }
}
